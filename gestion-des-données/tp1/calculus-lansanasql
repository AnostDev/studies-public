/*1*/SELECT QUANTITE FROM Vente;
/*2*/SELECT DISTINCT QUANTITE FROM Vente;
/*3*/SELECT QUANTITE FROM Vente WHERE QUANTITE = 20;
/*4*/SELECT QUANTITE FROM Vente WHERE QUANTITE = 60;
/*5*/SELECT QUANTITE FROM Vente WHERE PRIXVENTEUNITAIRE = 20;
/*6*/SELECT QUANTITE FROM Vente WHERE PRIXVENTEUNITAIRE = 60;
/*7*/SELECT QUANTITE FROM Vente WHERE QUANTITE > 20;
/*8*/SELECT QUANTITE FROM Vente WHERE PRIXVENTEUNITAIRE > 20;
/*9*/SELECT QUANTITE FROM Vente ORDER BY QUANTITE;
/*10*/SELECT QUANTITE FROM Vente ORDER BY PRIXVENTEUNITAIRE;
/*11*/SELECT COUNT( QUANTITE ) FROM Vente;
/*12*/SELECT COUNT( QUANTITE ) FROM Vente GROUP BY QUANTITE;
/*13*/SELECT QUANTITE, COUNT( QUANTITE ) FROM Vente GROUP BY QUANTITE;
/*14*/SELECT QUANTITE, COUNT( DISTINCT QUANTITE ) FROM Vente GROUP BY QUANTITE;
/*15*/SELECT QUANTITE, COUNT( PRIXVENTEUNITAIRE ) FROM Vente GROUP BY QUANTITE;
/*16*/SELECT QUANTITE, COUNT( DISTINCT PRIXVENTEUNITAIRE ) FROM Vente GROUP BY QUANTITE;
/*17*/SELECT PRIXVENTEUNITAIRE, COUNT( QUANTITE ) FROM Vente GROUP BY PRIXVENTEUNITAIRE;
/*18*/SELECT COUNT( QUANTITE ), COUNT( PRIXVENTEUNITAIRE ) FROM Vente;
/*19*/SELECT COUNT( QUANTITE ), COUNT( PRIXVENTEUNITAIRE ) FROM Vente GROUP BY QUANTITE, PRIXVENTEUNITAIRE;